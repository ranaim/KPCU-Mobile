<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="tests">	

  <test name="iPhone 12 Mini">
  		<listeners>
			<listener class-name="project.utils.TestListener" />
		</listeners>
		<groups>
		<run>
			<include name="regression"></include>
		</run>
		</groups>
		<parameter name="remoteOrLocal" value="Remote" />
		<parameter name="platformName" value="iOS" />
		<parameter name="automationName" value="UiAutomator2" />
		<parameter name="udid" value="replace_with_device_udid" />
		<parameter name="platformVersion" value="14" />
		<parameter name="deviceName" value="iPhone 12 Mini" />		
    <packages>
			<package name="project.tests"/>
	</packages>
  </test> 
  
  
