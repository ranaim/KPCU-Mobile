<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="tests">

   <test name="iPhone XS">
  		<listeners>
			<listener class-name="project.utils.TestListener" />
		</listeners>
		<parameter name="remoteOrLocal" value="Remote" />
		<parameter name="platformName" value="iOS" />
		<parameter name="automationName" value="UiAutomator2" />
		<parameter name="udid" value="replace_with_device_udid" />
		<parameter name="platformVersion" value="15.0" />
		<parameter name="deviceName" value="iPhone XS" />
    <classes>
       <class name="project.tests.Smoke_Tests"/>
    </classes>
  </test> 
       
 
 <test name="iPad Pro 12.9 2018">
  		<listeners>
			<listener class-name="project.utils.TestListener" />
		</listeners>
		<parameter name="remoteOrLocal" value="Remote" />
		<parameter name="platformName" value="iOS" />
		<parameter name="automationName" value="UiAutomator2" />
		<parameter name="udid" value="replace_with_device_udid" />
		<parameter name="platformVersion" value="15.0" />
		<parameter name="deviceName" value="iPad Pro 12.9 2018" />
    <classes>
       <class name="project.tests.Smoke_Tests"/>
    </classes>
  </test> 
       
</suite> 
 
 